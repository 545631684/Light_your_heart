.seve{
  width:523rpx;
  height:545rpx;
  background-color:white;
  border-radius:28rpx;
  position:fixed;
  top:317rpx;
  left:50%;
  margin-left:-261.5rpx;
  box-shadow:0px 0px 8px 4px rgba(203,203,203,0.26);
  z-index:1000;
}
.seve image{
  width:191rpx;
  height:191rpx;
  display:block;
  margin:72rpx auto;
}
.seve text{
  display:block;
  font-size:32rpx;
  font-family:'苹方-简 中黑体';
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:51rpx;
  margin-top:55rpx;
  text-align:center;
}
.seve view{
  width:214rpx;
  height:66rpx;
  margin:57rpx auto;
  border-radius:33rpx;
  border:1rpx solid rgba(34,88,236,1);
  font-size:28rpx;
  font-family:'苹方-简 常规体';
  font-weight:400;
  color:rgba(34,88,236,1);
  line-height:66rpx;
  text-align:center;
}