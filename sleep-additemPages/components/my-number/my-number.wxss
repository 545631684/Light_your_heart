/* pages/components/my-number/my-number.wxss */
.content{
  width: 100%;
  padding:20rpx 29rpx;
  border-bottom:1px solid #EDEDED;
}
.content1{
  width: 100%;
  padding:41rpx 29rpx;
}
.weightText{
  display: inline-block;
  width:8rpx;
  height:35rpx;
  background:rgba(28,82,240,1);
  border-radius:4px;
}
.weightText1{
  width:266rpx;
  height:45rpx;
  font-size:32rpx;
  font-family:PingFangHK-Semibold,PingFangHK;
  font-weight:600;
  color:rgba(51,51,51,1);
  line-height:45rpx;
  letter-spacing:1rpx;
  padding-left: 10rpx;
}
.bedSize{
  width:38rpx;
  height:41rpx;
}
.rightF{
  float: right;
  /* padding-right: 90rpx; */
  line-height: 50rpx;
  position: relative;
  font-size:32rpx;
  font-family:PingFangHK-Medium,PingFangHK;
  font-weight:500;
  color:rgba(28,82,240,1);
  line-height:50rpx;
  letter-spacing:1rpx;
}
.rightF text:nth-child(1){
  margin-right: 20rpx;
  font-size:32rpx;
  font-family:PingFangHK-Medium,PingFangHK;
  font-weight:500;
  color:rgba(28,82,240,1);
  line-height:45rpx;
  letter-spacing:1rpx;
}
.textBack{
  width:15rpx;
  height:27rpx;
  font-weight: 600;
  color:rgba(153,153,153,1);
  position: absolute;
  right: 0rpx;
  top:15rpx;
}
.lingText{
  position: relative;
  top:-5rpx;
  left: 10rpx;
  width:133rpx;
height:45rpx;
font-size:32rpx;
font-family:PingFangHK-Regular,PingFangHK;
font-weight:400;
color:rgba(51,51,51,1);
line-height:45rpx;
letter-spacing:1rpx;
}
/* 间隔 */
.interval{
  width: 100%;
  height:22rpx;
  background:rgba(244,245,247,1);
}
.container{
  position: fixed;
  z-index: 999;
  bottom:100rpx;
}
.contents{
  width:100%;
  height: 1000rpx;
  background-color: black;
  opacity: 0.5;
}
.page-body{
  width: 760rpx;
  height: 400rpx;
  background-color: white;
  
}
.colorGB{
  color:rgba(28,82,240,1);
}
.textend{
  position: fixed;
  text-align: center;
  width: 760rpx;
  font-size:32rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-family:PingFangHK-Regular,PingFangHK;
  font-weight:400;
  color:rgba(51,51,51,1);
  letter-spacing:1rpx;
  background-color: white;
}
.topY{
  margin-top: -100rpx;
  z-index: 1000;
}
