/* sleep-mainPages/pages/set/set.wxss */
.jiantou{
    height: 15rpx;
    width: 15rpx;
    border: 2rpx solid #7D7D7D;
    border-top: 1rpx solid transparent;
    border-left: 1rpx solid transparent;
    transform: rotate(-45deg);
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -5rpx;
}
.item{
    height: 117rpx;
    width:700rpx;
    border-top:1rpx solid #ededed ;
    position: relative;
    margin: 0 auto;
}
.item>text,navigator{  
    font-size: 34rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #333333;
    line-height: 117rpx;
}
.icon{
    width: 100%;  
    background-color:#F8F8F8 ;  
    padding: 25rpx;
    padding-bottom: 50rpx;
}
.icon .iconitem{
    width: 108rpx;
    display: inline-block;
    margin: 25rpx;
}
.icon .title{    
    font-size: 24rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #333333;
    line-height: 33rpx;
    width: 100%;
    text-align: center;
    margin-top: 20rpx;
}
.cancel{
    width: 100%;
    height: 100rpx;
    background: #FFFFFF;
    text-align: center;
    line-height: 100rpx;    
    font-size: 38rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    color: #333333;
}